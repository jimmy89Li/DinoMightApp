".container": {
	layout:"vertical",
	backgroundColor:"white"
}

"Label": {
	width: Ti.UI.SIZE,
	height: Ti.UI.SIZE,
	color: "#000"
}

"#label": {
	width: "85%",
	font: {
		fontSize: 12,
		textAlign: Titanium.UI.TEXT_ALIGNMENT_CENTER,
	}
}
"#ImageView": 
{
	height:"auto",
	width: "100%",
	image:"Incontinence.jpg"
}
"#User":
{
	layout:"vertical",
	height:"21%"
}
"#LoginRegister":
{
	layout:"horizontal",
	width: Ti.UI.SIZE,
	height:"10%"
	
}
"#Proceed":
{
	layout: "vertical"
}
"#username":{
	width: "50%", 
	height: 50,
	borderRadius: 10,
	top:10,
	color: 'black',
	borderStyle: Titanium.UI.INPUT_BORDERSTYLE_ROUNDED,
	borderColor: 'black',
	hintText: 'User Name',
	hintTextColor: "gray"
}

"#password": {
	width: "50%", 
	height: 50,
	borderRadius: 10,
	top: 10,
	color: 'black',
	borderStyle: Titanium.UI.INPUT_BORDERSTYLE_ROUNDED,
	borderColor: 'black',
	hintText: 'Password',
	hintTextColor: "gray",
	passwordMask: true
}
"#Btn_Login": {
	width: "40%", 
	borderRadius: 10,
	backgroundColor: "blue",
}
"#Btn_Register": {
	width: "40%", 
	left: 10,
	borderRadius: 10,
	backgroundColor: "blue",
}
"#Btn_Proceed":{
	width:"40%",
	top: 5,
	borderRadius: 10,
	backgroundColor: "blue"
}
